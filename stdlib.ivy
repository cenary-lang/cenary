pure int addvals(int x, int y) {
  return x + y;
}

int identity(int k) {
  return k;
}

char myid(char k) {
  return k;
}

int fib(int n) {
  int a = 0;
  int b = 1;
  int sum = 6;
  while (n > 1) {
    sum = addvals(a, b);
    a = b;
    b = sum;
    n = n - 1;
  };
  if(false) {
    sum = sum + 10;
  };
  return sum;
}
